function dot_product(v1, v2) {
  let product = 0;
  for (let i = 0; i < v1.length; i++) {
    product += v1[i] * v2[i];
  }
  return product;
}

for (let i = 0; i < vectors.length; i++) {
  for (let j = i + 1; j < vectors.length; j++) {
    if (dot_product(vectors[i], vectors[j]) === 0) {
      console.log(`${vectors[i]} and ${vectors[j]} are orthogonal `);
    }
  }
}
